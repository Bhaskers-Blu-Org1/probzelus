(env
 (dev
  (flags (:standard -w -9-27-33 -warn-error -A))))

(executable
 (name main)
 (libraries zelus core stdio str probzelus))

(rule
 (targets gender_bias.ml main.ml)
 (deps    (:src gender_bias.zls) utils.zci)
 (action  (run zeluc -copy -I ../../../../inference -s main %{src})))

(rule
 (targets utils.zci)
 (deps (:src utils.zli))
 (action (run zeluc -copy %{src})))
